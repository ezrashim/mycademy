= form_for @course do |f|
  .div.input-field
    = f.label :title, "Title"
    = f.text_field :title
  .div.input-field
    = f.label :description, "Description"
    = f.text_field :description

  .div.input-field
    = f.label :passcode, "Passcode"
    = f.text_field :passcode

  .waves-effect.secondary-content.btn-large
    = f.submit
